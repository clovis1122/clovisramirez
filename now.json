{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/$1" },
        { "handle": "filesystem" },
        { "src": "/.*", "status": 404, "dest": "/404" }
      ]
  }
